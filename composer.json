{
  "name": "phactor/doctrine",
  "description": "Doctrine based persistence for phactor",
  "minimum-stability": "stable",
  "license": "MIT",
  "require": {
    "php": "^7.4 | ^8.0",
    "doctrine/orm": "^2.0.0",
    "phactor/phactor": "2.0.x-dev",
    "jms/serializer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Phactor\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhactorTest\\": "test\\unit",
      "PhactorTestMocks\\": "test\\mocks"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
